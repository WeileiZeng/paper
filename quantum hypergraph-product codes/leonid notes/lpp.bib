@Article{Naud-00-prl,
  author =       {Naud, J. D. and Pryadko, L. P. and Sondhi, S. L.},
  title =        {Fractional quantum {H}all effect in infinite-layer
                  systems},
  journal =      prl,
  year =         2000,
  month =        {Dec.},
  volume =       85,
  number =       25,
  pages =        {5408-11},
  url =          {http://link.aps.org/abstract/PRL/v85/p5408},
  annote =       {Stacked two dimensional electron systems in
                  transverse magnetic fields exhibit three dimensional
                  fractional quantum {H}all phases. We analyze the
                  simplest such phases and find novel bulk properties,
                  e.g., irrational braiding. These phases host "one
                  and a half" dimensional surface phases in which
                  motion in one direction is chiral. We coffer a
                  general analysis of conduction in the latter by
                  combining sum rule and renormalization group
                  arguments, and find that when interlayer tunneling
                  is marginal or irrelevant they are chiral semimetals
                  that conduct only at T>0 or with disorder.}
}

