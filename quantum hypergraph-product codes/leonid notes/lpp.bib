@article{pryadko-quiroz-2009,
  author =	 {L. P. Pryadko and Gregory Quiroz},
  title =	 {Soft-pulse dynamical decoupling with {M}arkovian
                  decoherence},
  year =	 2009,
  journal =	 {Phys. Rev. A},
